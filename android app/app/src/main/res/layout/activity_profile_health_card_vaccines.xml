<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileHealthCardVaccinesVM" type="com.happytails.app.modules.profilehealthcardvaccines.data.viewmodel.ProfileHealthCardVaccinesVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearProfileHealt"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearNavigationHead"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_arrowleft_bluegray_401}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_401"/>
                <View android:background="@color/bluegray_50"
                    android:id="@+id/lineLineDivider"
                    android:layout_height="@dimen/_22pxv"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.0" android:layout_width="0dp"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnbody"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.65"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBody"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtBody}"
                        style="@style/txtNotoSansnotosansregular14" tools:text="@string/lbl_pet_profile"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTitle"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_11pxh"
                        android:singleLine="true"
                        android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtTitle}"
                        style="@style/txtCatamaranromansemibold16" tools:text="@string/lbl_vaccines"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowphotoplaceim"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_weight="0.3"
                    android:layout_width="0dp"
                    android:orientation="horizontal" style="@style/groupStylebluegray_50cornerRadius">
                    <ImageView android:id="@+id/imagePhotoPLACEIM"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_14pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_photoplaceim_20x20}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_photoplaceim_20x20"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtText"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.65"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_6pxh"
                        android:singleLine="true"
                        android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtText}"
                        style="@style/txtNotoSansnotosansmedium14_1" tools:text="@string/lbl_balaji"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <View
            android:background="@drawable/rectangle_gradient_s_bluegray_400_87_e_bluegray_400_87"
            android:id="@+id/lineLine"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_14pxv" android:layout_width="@dimen/_327pxh"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearSearchInput"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearInputBox"
                android:layout_height="wrap_content"
                android:layout_weight="0.83" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylecornerRadius_2">
                <androidx.appcompat.widget.SearchView
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_301_radius_14"
                    android:gravity="start"
                    android:id="@+id/searchViewInputContent"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:theme="@style/searchViewInputContent"
                    app:iconifiedByDefault="false"
                    app:queryBackground="@android:color/transparent"
                    app:queryHint="@string/msg_search_by_vacci" app:searchIcon="@drawable/img_search"/>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_blue_600_radius_14"
                android:id="@+id/btnSend"
                android:layout_height="@dimen/_54pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_width="@dimen/_54pxh"
                android:paddingHorizontal="@dimen/_20pxh"
                android:paddingVertical="@dimen/_20pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_send" tools:ignore="ContentDescription"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtZipcode"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:singleLine="true"
            android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtZipcode}"
            style="@style/txtNotoSansnotosanssemibold16_2" tools:text="@string/lbl_2024"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearCardHorizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_9">
            <TextView android:ellipsize="end"
                android:id="@+id/txtTitleOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtTitleOne}"
                style="@style/txtNotoSansnotosanssemibold14" tools:text="@string/lbl_nobivac_parvo_c"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearBody"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_height="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar_bluegray_202"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar_bluegray_202"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt11022024"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.38"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txt11022024}"
                    style="@style/txtNotoSansnotosansmedium14_3" tools:text="@string/lbl_11_02_2024"/>
                <View android:background="@color/gray_301"
                    android:id="@+id/lineDivider"
                    android:layout_height="@dimen/_15pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.01" android:layout_width="0dp"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTextOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_1pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.55"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtTextOne}"
                    style="@style/txtNotoSansnotosansmedium14_3" tools:text="@string/lbl_dr_martha_roth"/>
            </LinearLayout>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtZipcodeOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:singleLine="true"
            android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtZipcodeOne}"
            style="@style/txtNotoSansnotosanssemibold16_2" tools:text="@string/lbl_2023"/>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListtitleTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_listtitle_two"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtZipcodeTwo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:singleLine="true"
            android:text="@{profileHealthCardVaccinesVM.profileHealthCardVaccinesModel.txtZipcodeTwo}"
            style="@style/txtNotoSansnotosanssemibold16_2" tools:text="@string/lbl_2022"/>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListtitleFive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_listtitle_five"/>
    </LinearLayout>
</layout>
