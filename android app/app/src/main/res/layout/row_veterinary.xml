<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="veterinaryRowModel" type="com.happytails.app.modules.veterinary.data.model.VeterinaryRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearDoctor"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
        <ImageView android:id="@+id/imageImageFive"
            android:layout_height="@dimen/_90pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_90pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_8pxh}"
            imageRes="@{@drawable/img_image_90x90}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_90x90"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumndrmarianai"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_weight="0.68" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtDrMariaNai"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{veterinaryRowModel.txtDrMariaNai}"
                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_dr_billy_maria"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtVeterinaryDent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{veterinaryRowModel.txtVeterinaryDent}"
                style="@style/txtPoppinsregular12" tools:text="@string/msg_veterinary_beha"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsignal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSignal"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_signal"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_signal"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFortyNine"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.26"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{veterinaryRowModel.txtFortyNine}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_4_8"/>
                <ImageView android:id="@+id/imageFimappinOne"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_fimappin"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_fimappin"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDistance"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_92pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_weight="0.38"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{veterinaryRowModel.txtDistance}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_1_km"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
