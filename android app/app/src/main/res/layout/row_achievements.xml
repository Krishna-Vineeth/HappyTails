<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="achievementsRowModel" type="com.happytails.app.modules.achievements.data.model.AchievementsRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowmonotoneapple"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearHealthAssessme1"
            android:layout_height="wrap_content"
            android:layout_weight="0.5" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStylewhite_12">
            <ImageView android:id="@+id/imageMonotoneapple"
                android:layout_gravity="end"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="@dimen/_16pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_monotoneapple"
                tools:ignore="ContentDescription" tools:src="@drawable/img_monotoneapple"/>
            <TextView android:gravity="start"
                android:id="@+id/txtBalancedDiet"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_102pxv"
                android:layout_width="match_parent"
                android:text="@{achievementsRowModel.txtBalancedDiet}"
                style="@style/txtPlusJakartaSansplusjakartasanssemibold16" tools:text="@string/msg_donation_you_ma"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearHealthAssessme2"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_weight="0.5" android:layout_width="0dp"
            android:orientation="vertical" style="@style/groupStylelight_blue_4">
            <ImageView android:id="@+id/imageSettingsTwo"
                android:layout_gravity="end"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="@dimen/_18pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_settings_white_a700_16x18"
                tools:ignore="ContentDescription" tools:src="@drawable/img_settings_white_a700_16x18"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtBalancedDietOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_125pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{achievementsRowModel.txtBalancedDietOne}"
                style="@style/txtPlusJakartaSansplusjakartasanssemibold16_1" tools:text="@string/lbl_adopted_pet_s"/>
        </LinearLayout>
    </LinearLayout>
</layout>
