<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrectangle1imRowModel" type="com.happytails.app.modules.community.data.model.Listrectangle1imRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangle1im"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageRectangle1im"
            android:layout_height="@dimen/_55pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="@dimen/_56pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_19pxh}"
            imageRes="@{@drawable/img_rectangle1im}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1im"/>
        <LinearLayout android:id="@+id/linearRowconnorfrazier"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_weight="0.84" android:layout_width="0dp" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnconnorfrazier"
                android:layout_height="wrap_content"
                android:layout_weight="0.85" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConnorFrazier"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:singleLine="true"
                    android:text="@{listrectangle1imRowModel.txtConnorFrazier}"
                    style="@style/txtPoppinsregular16_1" tools:text="@string/lbl_trent_will"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.02"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:text="@{listrectangle1imRowModel.txtDescription}"
                    style="@style/txtPoppinsmedium12_3" tools:text="@string/msg_louis_is_always"/>
            </LinearLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearColumntime"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.15" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:gravity="start" android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{listrectangle1imRowModel.txtTime}"
                    style="@style/txtPoppinsbold12" tools:text="@string/lbl_7_11_pm"/>
                <FrameLayout android:id="@+id/frameStackrectanglefour"
                    android:layout_height="@dimen/_20pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_25pxh">
                    <View
                        android:background="@drawable/rectangle_bg_blue_a400_radius_6"
                        android:id="@+id/viewRectangleFour"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_20pxv"
                        android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_25pxh"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtWhatkindofmu"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_7pxh"
                        android:singleLine="true"
                        android:text="@{listrectangle1imRowModel.txtWhatkindofmu}"
                        style="@style/txtPoppinsblack14" tools:text="@string/lbl_16"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
