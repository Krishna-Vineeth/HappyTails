<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="veterinaryVM" type="com.happytails.app.modules.veterinary.data.viewmodel.VeterinaryVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnfimappin"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_1pxv"
        android:layout_marginEnd="@dimen/_25pxh"
        android:layout_marginStart="@dimen/_26pxh"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowfimappin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFimappin"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_fimappin"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_fimappin"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCountry"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{veterinaryVM.veterinaryModel.txtCountry}"
                    style="@style/txtPoppinsmedium14" tools:text="@string/msg_hyderabad_indi"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearVeterinary"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_100">
            <LinearLayout android:id="@+id/linearBanner"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylelight_blue_300cornerRadius">
                <TextView android:id="@+id/txtInLoveWithPe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_weight="0.69"
                    android:layout_width="0dp"
                    android:text="@{veterinaryVM.veterinaryModel.txtInLoveWithPe}"
                    style="@style/txtPoppinssemibold16" tools:text="@string/msg_lets_find_speci"/>
                <ImageView android:id="@+id/imageImage"
                    android:layout_height="@dimen/_67pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_99pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="@dimen/_71pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_image}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
            </LinearLayout>
            <EditText
                android:background="@drawable/rectangle_border_light_blue_300_radius_8"
                android:drawableEnd="@drawable/img_fisearch"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end"
                android:hint="@string/lbl_search"
                android:id="@+id/etTeksField" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@={veterinaryVM.veterinaryModel.etTeksFieldValue}"
                android:textColorHint="@color/gray_400"
                style="@style/etRoundedOutline" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowourservices"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOurServices"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.69"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{veterinaryVM.veterinaryModel.txtOurServices}"
                    style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_our_services"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSeeAll"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_230pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{veterinaryVM.veterinaryModel.txtSeeAll}"
                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_see_all"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowvaccinations"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnvaccinations"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.3"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_width="@dimen/_54pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image_54x54}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_54x54"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtVaccinations"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{veterinaryVM.veterinaryModel.txtVaccinations}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_vaccinations"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnoperations"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.27"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_width="@dimen/_54pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image_1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_1"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtOperations"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{veterinaryVM.veterinaryModel.txtOperations}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_operations"/>
                </LinearLayout>
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnbehaviorals"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.28"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageThree"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_marginEnd="@dimen/_37pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_width="@dimen/_54pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image_2}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_2"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtBehaviorals"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{veterinaryVM.veterinaryModel.txtBehaviorals}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_behaviorals"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumndentistry"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.15"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageImageFour"
                        android:layout_height="@dimen/_54pxh"
                        android:layout_width="match_parent"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_image_3}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_3"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDentistry"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{veterinaryVM.veterinaryModel.txtDentistry}"
                        style="@style/txtPoppinsmedium12" tools:text="@string/lbl_dentistry"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBestSpecialist"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{veterinaryVM.veterinaryModel.txtBestSpecialist}"
                style="@style/txtPoppinsmedium14_1" tools:text="@string/msg_best_specialist"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerVeterinary"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="3" tools:listitem="@layout/row_veterinary"/>
        </LinearLayout>
    </LinearLayout>
</layout>
