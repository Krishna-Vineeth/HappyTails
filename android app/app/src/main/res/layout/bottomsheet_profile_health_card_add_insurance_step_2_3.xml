<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileHealthCardAddInsuranceStep23VM" type="com.happytails.app.modules.profilehealthcardaddinsurancestep23.data.viewmodel.ProfileHealthCardAddInsuranceStep23VM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearProfileHealt"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearActionSheets"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50cornerRadius">
            <LinearLayout android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_41pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_4pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_arrowleft_bluegray_401}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_bluegray_401"/>
                <LinearLayout android:id="@+id/linearRowtitle"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_96pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumntitle"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.73"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtTitle"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_11pxh"
                            android:singleLine="true"
                            android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtTitle}"
                            style="@style/txtCatamaranromansemibold16" tools:text="@string/lbl_add_insurance2"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBody"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtBody}"
                            style="@style/txtNotoSansnotosansregular14" tools:text="@string/lbl_package_details"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearStep"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_73pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.27"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtBodyOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_4pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_6pxh"
                            android:singleLine="true"
                            android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtBodyOne}"
                            style="@style/txtNotoSansnotosansregular14_1" tools:text="@string/lbl_step"/>
                        <LinearLayout android:gravity="center_vertical"
                            android:id="@+id/linearStep1"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginStart="@dimen/_7pxh"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtBoldBody"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtBoldBody}"
                                style="@style/txtNotoSansnotosanssemibold14" tools:text="@string/lbl_22"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFour"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtFour}"
                                style="@style/txtNotoSansnotosansregular14_2" tools:text="@string/lbl6"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtLightBody"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="wrap_content"
                                android:lineSpacingExtra="@dimen/_6pxh"
                                android:singleLine="true"
                                android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtLightBody}"
                                style="@style/txtNotoSansnotosansregular14_2" tools:text="@string/lbl_32"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageProgressBar"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_2pxh"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="@dimen/_322pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_progressbar"
                tools:ignore="ContentDescription" tools:src="@drawable/img_progressbar"/>
            <TextView android:ellipsize="end" android:id="@+id/txtText"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{profileHealthCardAddInsuranceStep23VM.profileHealthCardAddInsuranceStep23Model.txtText}"
                style="@style/txtNotoSansnotosanssemibold16_2" tools:text="@string/lbl_comfy_pack"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListbodyTwo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_22pxh}" tools:itemCount="6" tools:listitem="@layout/row_listbody_two"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearButtonsContain"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_81pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_7">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_height="match_parent"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:paddingBottom="@dimen/_17pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_3" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
